{
  "name": "loopback",
  "version": "3.35.4",
  "description": "LoopBack: Open Source Framework for Node.js",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "author": "IBM Corp.",
  "homepage": "http://loopback.io",
  "keywords": [
    "web",
    "restful",
    "rest",
    "api",
    "express",
    "restify",
    "koa",
    "auth",
    "security",
    "oracle",
    "mysql",
    "nosql",
    "mongo",
    "mongodb",
    "sqlserver",
    "mssql",
    "postgres",
    "postgresql",
    "soap",
    "StrongLoop",
    "framework",
    "mobile",
    "mBaaS"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "grunt eslint",
    "coverage": "c8 report",
    "report-coverage": "c8 report --reporter=lcov && cat ./coverage/lcov.info | coveralls",
    "test": "c8 mocha test/**/*.js --exit --timeout 5000 --color --reporter spec",
    "test:compatibility": "mocha test/express-compatibility.test.js --timeout 15000 --color --reporter spec",
    "test:performance": "mocha test/middleware-performance.test.js --timeout 15000 --color --reporter spec",
    "test:concurrency": "mocha test/middleware-concurrency.test.js --timeout 20000 --color --reporter spec",
    "test:middleware": "npm run test:compatibility && npm run test:performance && npm run test:concurrency",
    "reinstall": "rm -rf node_modules/ && rm yarn.lock && yarn install"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "debug": "^4.4.1",
    "depd": "^2.0.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "fast-json-stable-stringify": "^2.1.0",
    "inflection": "2.0.1",
    "isemail": "^3.2.0",
    "loopback-connector-remote": "^3.4.1",
    "loopback-datasource-juggler": "github:perkd/loopback-datasource-juggler#semver:^5.2.7",
    "loopback-filters": "^1.1.1",
    "loopback-phase": "3.4.0",
    "nodemailer": "^7.0.5",
    "nodemailer-direct-transport": "^3.3.2",
    "nodemailer-stub-transport": "^1.1.0",
    "serve-favicon": "^2.5.1",
    "stable": "^0.1.8",
    "strong-globalize": "^6.0.6",
    "strong-remoting": "github:perkd/strong-remoting#semver:^3.20.3",
    "uid2": "1.0.0",
    "underscore.string": "^3.3.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "browserify": "^17.0.1",
    "c8": "^10.1.3",
    "chai": "4.2.0",
    "cookie-parser": "^1.4.7",
    "coveralls": "^3.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^9.30.1",
    "eslint-config-loopback": "^13.1.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-mocha": "^11.1.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-security": "^3.0.1",
    "express-session": "^1.18.1",
    "grunt": "^1.6.1",
    "grunt-browserify": "^6.0.0",
    "grunt-cli": "^1.5.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^25.0.0",
    "grunt-mocha-test": "^0.13.3",
    "is-docker": "2.2.1",
    "loopback-boot": "^3.3.1",
    "loopback-context": "3.5.2",
    "mocha": "^11.7.1",
    "sinon": "^21.0.0",
    "sinon-chai": "3.7.0",
    "strong-error-handler": "^5.0.20",
    "strong-task-emitter": "^0.0.8",
    "supertest": "^7.1.3",
    "which": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback"
  },
  "browser": {
    "./lib/server-app.js": "./lib/browser-express.js",
    "bcrypt": false,
    "connect": false,
    "depd": "loopback-datasource-juggler/lib/browser.depd.js",
    "express": "./lib/browser-express.js",
    "nodemailer": false,
    "supertest": false
  },
  "config": {
    "ci": {
      "debug": "*,-mocha:*,-eslint:*"
    }
  },
  "ci": {
    "downstreamIgnoreList": [
      "bluemix-service-broker",
      "gateway-director-bluemix",
      "plan-manager"
    ]
  },
  "packageManager": "yarn@4.9.2"
}
