{
  "all": true,
  "include": [
    "server/**/*.js",
    "lib/**/*.js"
  ],
  "exclude": [
    "test/**",
    "coverage/**",
    "node_modules/**"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
} 